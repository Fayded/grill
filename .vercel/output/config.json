{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/_actions(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/email-newsletter\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/send-email\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/catering/?$",
			"dest": "_render"
		},
		{
			"src": "^/location/?$",
			"dest": "_render"
		},
		{
			"src": "^/menu/?$",
			"dest": "_render"
		},
		{
			"src": "^/thank-you/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}